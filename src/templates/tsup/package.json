{
  "name": "tsup-test-v2",
  "version": "1.0.1",
  "description": "",
  "private": false,
  "access": "public",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "test": "vitest --passWithNoTests",
    "dev": "tsx watch ./src/index.ts",
    "coverage": "vitest run --coverage --passWithNoTests",
    "build": "tsup ./src/index.ts --dts --format cjs,esm",
    "lint": "tsc",
    "publish": "npm run ci && changeset publish",
    "ci": "npm run lint && npm run coverage && npm run build",
    "bump": "changeset"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrienne-deriv/test-tsup-v2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adrienne-deriv/test-tsup-v2/issues"
  },
  "homepage": "https://github.com/adrienne-deriv/test-tsup-v2#readme",
  "devDependencies": {
    "@vitest/coverage-v8": "^1.3.1",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
